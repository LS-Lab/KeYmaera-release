\functions {
  R b;
  R pzone;
  R T;
  R x1;
  R y1;
  R x2;
  R y2;
}

\problem {
 b >= 0 &
 pzone >= 0 &
 T >= 0 &
 (x1 - y1) ^ 2 + (x2 - y2) ^ 2 >= (pzone + 2 * b * T) ^ 2
->
 \forall R d2_0; 
   \forall R d1_0; 
     \forall R x1_0; 
       \forall R x2_0; 
         \forall R e2_0; 
           \forall R e1_0; 
             \forall R y1_0; 
               \forall R y2_0; 
                 \forall R t_0; 
                   (     t_0 <= T
                       & d1_0 ^ 2 + d2_0 ^ 2 <= b ^ 2
                       & e1_0 ^ 2 + e2_0 ^ 2 <= b ^ 2
                       & -t_0 * b <= x1_0 - x1
                       & x1_0 - x1 <= t_0 * b
                       & -t_0 * b <= x2_0 - x2
                       & x2_0 - x2 <= t_0 * b
                       & -t_0 * b <= y1_0 - y1
                       & y1_0 - y1 <= t_0 * b
                       & -t_0 * b <= y2_0 - y2
                       & y2_0 - y2 <= t_0 * b
                    -> (x1_0 - y1_0) ^ 2 + (x2_0 - y2_0) ^ 2 >= pzone ^ 2)
 }