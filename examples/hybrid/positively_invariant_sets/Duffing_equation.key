(-641 + 2*x1 + 10969*x1^2 + 12*x1^3 - 20780*x1^4 - 13*x1^5 + 10000*x1^6 - 5*x2 - 41291*x1*x2 + 17*x1^2*x2 + 83659*x1^3*x2 + 4*x1^4*x2 - 40639*x1^5*x2 + 49039*x2^2 - 148*x1*x2^2 - 102496*x1^2*x2^2 + 51*x1^3*x2^2 + 66980*x1^4*x2^2 + 115*x2^3 - 248650*x1*x2^3 - 18*x1^2*x2^3 + 98766*x1^3*x2^3 + 397279*x2^4 - 362*x1*x2^4 + 10911*x1^2*x2^4 + 1022*x2^5 - 11562*x1*x2^5 + 64226*x2^6  <= 0)
