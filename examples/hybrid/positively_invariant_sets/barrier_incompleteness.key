/**
 * Incompleteness of barrier certificates
 * Example adapted from Bhat&Bernstein
 * s0805753@sms.ed.ac.uk, University of Edinburgh, 12/08/13.
 * x1,x2 : state variables

 */
\functions {
   R c;
   \external R Sqrt(R);
   \external R E;
   \external R Exp(R);
   \external R Sin(R);
   \external R Cos(R);
   \external R Tan(R);
   \external R ArcSin(R);
   \external R ArcCos(R);
   \external R ArcTan(R);
   \external R Sinh(R);
   \external R Cosh(R);
   \external R Tanh(R);

} 
\programVariables{
   R x1;
   R x2;
}

\problem {
 (  
    (x1^2+x2^2-1)^2*(x1^2 +x2^2 - 1/2)^3 <=0 -> 
         \[ {  x1' = -x1 - x2 +x1*x2^2 +x1^3,
               x2' = x1 -x2 +x1^2*x2 +x2^3
            } \] 
    (x1^2+x2^2 -1)^2*(x1^2 +x2^2 - 1/2)^3 <=0 
 )
}
