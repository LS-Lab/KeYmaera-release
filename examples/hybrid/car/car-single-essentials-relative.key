/**
 * Essentials of single car relative version
 * @provable (and trivial when instantiating one antecedent quantifier by variable for succedent quantifier)
 */
\functions{
  R ep;
  R b;
  R A;
}
\programVariables {
R a, v, x, t, m;
}

/* m-x>=(v^2)/(2*b) + ((A/b) + 1)*((A/2)*ep^2 + ep*v */

\problem {
    ( b > 0 & A>=0 & v>=0 & \[{x'=v, v' = -b}\] (x <= m))
  -> 
   \[(
    ((a:= -b) ++ (?2*b*(m-x)>=v^2 + (A + b)*(A*ep^2 + 2*ep*v); a:=A));
    (t:=0; {x'=v, v' = a, t'=1, (v >= 0 & t <= ep)})
    )*@invariant(v^2<=2*b*(m-x))\] (x <= m)
}
