\programVariables {
R x1, v1, a1 ;
R x2, v2, a2, x2r, v2r, t ;
R b, B, A, ep, Tr;
}


\problem{
\[ b := B
  \] ( 
 
    x2 - v2r*Tr - A/2*Tr^2 <= x2r 
  & x2r <= x2
  & v2 - A*Tr <= v2r
  & v2r <= v2 + B*Tr 
  & (x2r > x1 + v1 ^ 2 / (2 * b) - (v2r^2 - 2*v2r*B*Tr + B^2*Tr^2)/ (2 * B) + (A / b + 1) * (A/2 * ep ^ 2 + ep * v1) & v2r-b*Tr >= 0 
     | x2r > x1 + v1 ^ 2 / (2 * b) + (A / b + 1) * (A/2 * ep ^ 2 + ep * v1)
     )
  & B >= b
  & b >  0
  & v1 >= 0         
  & v2 >= 0
  & A >= 0
  & ep >  0
  & Tr < ep
  & Tr > 0
  & v2r >= 0

  ->
  x2 > x1 + v1 ^ 2 / (2 * b) - v2 ^ 2 / (2 * B) + (A / b + 1) * (A / 2 * ep ^ 2 + ep * v1)
)
}
