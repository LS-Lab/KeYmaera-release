\programVariables {
  R x, v, a, t; 
  R b, A, ep, m;
}

/**
 * @provable
 * Variable Definitions:
 * x position of car
 * v velocity of car
 * a acceleration of car
 * A max acceleration 
 * b max braking 
 * m position of Stop Sign
 */


\problem {
         \[
           {x' = v, v' = -(b)}
           \] x <= m
       & v >= 0
     & A >= 0
   & b >  0
-> \[
     ((?([t := 0 ;
           {x' = v, v' = A, 
           t' = 1, t <= ep}][{
           x' = v, v' = -(b)}]x <= m) ;
         a := A)
       ++ a := -(b) ;
       (t := 0 ;
         {x' = v, v' = a, 
         t' = 1, v >= 0, t <= ep}))*
     \] x <= m

}

