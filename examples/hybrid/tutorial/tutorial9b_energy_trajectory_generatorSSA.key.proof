/* 
* SSA example with a trajectory generator for the PD controller,
* so that car inches forward towards the intersection.
*
* Should prove automatically
*
* Author: Nikos Arechiga
*/

\problem {
\[
  R p, v, a, S, Kp, Kd, c, p_r, pmeasured, t, tau
  \] (   (  (  (  (  (  (  (((v) >= (0)) & ((Kp) = (2)))
                         & ((Kd) = (3)))
                      & ((tau) >  (0)))
                   & ((c) >  (0)))
                & ((pmeasured) = (p)))
             & (  (p_r)
                = (((pmeasured) / (2)) + ((S) / (2)))))
          & (   (  (  (  ((5) / (4))
                       * (((p) - (p_r)) ^ (2)))
                    + ((((p) - (p_r)) * (v)) / (2)))
                 + (((v) ^ (2)) / (4)))
             <  (  (((S) / (2)) - ((pmeasured) / (2)))
                 ^ (2))))
      -> (\[
            ((pmeasured := (p)) ;
