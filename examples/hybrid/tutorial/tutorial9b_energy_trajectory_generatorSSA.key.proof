          & (   (  (  (  ((5) / (4))
                       * (((p) - (ref)) ^ (2)))
                    + ((((p) - (ref)) * (v)) / (2)))
                 + (((v) ^ (2)) / (4)))
             <  (  (((S) / (2)) - ((pmeasured) / (2)))
                 ^ (2))))
      -> (\[
