\[
  R p, v, a, S, Kp, Kd, c, ref, pmeasured, t, tau
  \] (   (  (  (  (  (  (  (((v) >= (0)) & ((Kp) = (2)))
                         & ((Kd) = (3)))
                      & ((tau) >  (0)))
                   & ((c) >  (0)))
                & ((pmeasured) = (p)))
             & (  (ref)
