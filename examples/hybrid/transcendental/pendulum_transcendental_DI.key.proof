/* s0805753@sms.ed.ac.uk, University of Edinburgh.
 * 
 * DESCRIPTION
 *   This example provides an estimate to the domain of attraction of a 
 *   whirling pendulum, as presented by Graziano Chesi in "Estimating 
 *   the Domain of Attraction for Non-Polynomial Systems via LMI 
 *   optimizations", University of Hong Kong.
 *   
 *   This example is provable using differential induction DI.
 */

\functions {
\external R E;
\external R Exp(R);
\external R Sin(R);
\external R Cos(R);
\external R Tan(R);
R a;
}

\programVariables {
R x1, x2, kf, mb, omega, g, lp;
}

\problem {
         kf = 0.2 & mb = 1 & omega = 0.9 & g = 10
       & lp = 10
     & x1 ^ 2 + x1 * x2 + 4 * x2 ^ 2 <= a
   & a >= 0
-> \[
     {x1' = x2, x2' = (-(kf / mb) * x2 + (omega ^ 2 * Sin(x1)) * Cos(x1)) - g / lp * Sin(x1), 
       (x1 ^ 2 + x1 * x2) + 4 * x2 ^ 2 <= 0.69929971}
     \] x1 ^ 2 + x1 * x2 + 4 * x2 ^ 2 <= a

}

\proof {
(keyLog "0" (keyUser "s0805753" ) (keyVersion "43dad945567b1cdd22504434eb1336257ccf38ac"))

(autoModeTime "0")

(nodes "5")

(branches "2")

(interactiveSteps "4")

(branch "dummy ID"
(rule "imp_right" (formula "1") (userinteraction) (nodenum "25231"))
(rule "diffind" (formula "2") (userinteraction) (nodenum "25309"))
(branch " Invariant Initially Valid"
   (builtin "Eliminate Universal Quantifiers" (nodenum "26059"))
)
(branch " ODE Preserves Invariant"
   (builtin "Update Simplification" (formula "2") (nodenum "25749"))
   (builtin "Eliminate Universal Quantifiers" (reduceVariables "x1_0, x2_0") (nodenum "26066"))
)
)

}
