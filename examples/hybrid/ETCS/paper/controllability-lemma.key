/**
 * Braking mode of European Train Control System (ETCS)
 */
\sorts{
  R;
}

\functions{
  R b;
  R m;
  R d;
}

/* could assumptions be relieved to z<=m etc? */
\problem {
  \[ R v; R z \] (v >= 0 & d >= 0 & b>0 & z<=m -> (
  (
    /*( z < m)
  ->*/
   \[
    {z`=v, v` = -b, (v >= 0)}
    \] (z >= m -> v <= d)
  ) <->
  (/*v>=0 & z<m ->*/ v^2-d^2<=2*b*(m-z))
  )
  )
}
