\functions{
  R ep;
  R b;
  R A;
  R v;
  R m;
  R z;
}

\problem {
\forall R v_1; \forall R z_1;
(

(    m
  - z_1
 >= (v_1) ^ 2 / (2 * b)
  + (A / b + 1) * (A / 2 * (ep) ^ 2 + ep * v_1)&
 (v_1) ^ 2 <= 2 * b * (m - z_1)&
 v ^ 2 <= 2 * b * (m - z)&
 b >  0&
 A >= 0)
-> (
 \forall R t2; 
   (   t2 >= 0
    -> \forall R ts2; 
         (0 <= ts2 & ts2 <= t2 -> A * ts2 + v_1 >= 0 & ts2 + 0 <= ep)
    ->    (A * t2 + v_1) ^ 2
       <= 2 * b * (m - 1 / 2 * (A * t2 ^ 2 + 2 * t2 * v_1 + 2 * z_1))))
)
 
}
