/**
 * @see Andre Platzer. Logical Analysis of Hybrid Systems: Proving Theorems for Complex Dynamics. Counterexample 3.17 variation
 * @false
 */
\programVariables {
  R x1;
  R x2;
  R d1;
  R d2;
  R om;
}

\problem {
  (x1>=0 | d1^2+d2^2=v^2) & d1^2+d2^2=1 & om!=0 -> \[{x1'=d1,x2'=d2,d1'=-om*d2,d2'=om*d1}\](x1>=0 | d1^2+d2^2=v^2)
}