/**
 * Inverted Pendulum on Cart
 *===========================
 *
 * 1-D no-friction model of inverted pendulum on a cart.
 * Safety condition phrased in terms of pitch angle.
 *
 * 
 */
 
\functions {
	R Theta;
	R g;
	R l;
}

\programVariables {
	R w;
	R theta;
	R f;
	R c;
	R s;
}

\problem {
   (  (  (  (  (  (((Theta) >  (0)) & ((g) >  (0)))
                & ((l) = (1)))
             & ((theta) = (0)))
          & ((w) = (0)))
       & ((c) = (1)))
    & ((((c) ^ (2)) + ((s) ^ (2))) = (1)))
-> (\[
      (f := (((-(g)) / (l)) * (s))) ;
        ({(theta') = (w), (w') = ((f) + (((g) / (l)) * (s))), 
          (s') = ((w) * (c)), 
          (c') = ((-(w)) * (s))})
      \] (  ((-(Theta)) <= (theta))
          & ((theta) <= (Theta))))

}

