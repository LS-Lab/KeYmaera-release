\programVariables {
  R Theta,b,a,z,w,theta,r,w0,theta0,z0;
}

\problem {
  z0 > 0
  & z0*(-w0^2 + b*w0*(-r + theta0) + a*(-r + theta0)^2) = z*(-w^2 + b*w*(theta - r) + a*(theta - r)^2) 
  & z0 >= z 
  & a < 0 
  & b^2 < -4*a 
  & b < 0
  & (b^2 + 4*a)*(Theta + r)^2 < 4*(-w^2 + b*w*(theta - r) + a*(theta - r)^2) 
  & Theta + r > 0 
  ->
  (r>=0 | theta0 <= Theta)
}

