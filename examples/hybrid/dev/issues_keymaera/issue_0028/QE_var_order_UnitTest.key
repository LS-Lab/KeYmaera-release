/*
http://pdf.aminer.org/000/949/736/on_the_combinatorial_and_algebraic_complexity_of_quantifier_elimination.pdf  
 */

\programVariables {
  R a,x1,x2,y1,y2,z1,z2,x0,alpha,y0,x;
}


\problem {
/*  a<0 & x1<0 & x2>0 & y1>0 & y2<0 & z1<0 & z2>0 & x0<1 & alpha>2 & y0>2 & x<0
  -> */
  ((y1-1/2)^2+(x1-z2)^2)*((y1-z2)^2+(x1-x2)^2)*x^3+((y0-y1)^2+(x0-z1)^2)*((y0-z1)^2+(x0-x1)^2)*x^2+((y0-2*x0)^2+(alpha^2+(x0-1/2))^2)*((y0-2+2*x0)^2+(alpha^2-(x0-1/2))^2)*x + a > 0
  ->
  x<0 & z2^2>2
}

