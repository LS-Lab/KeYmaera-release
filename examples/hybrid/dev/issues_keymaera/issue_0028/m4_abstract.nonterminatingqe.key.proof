/*
 * Inverted Pendulum on Cart
 *===========================
 *
 * 1-D no-friction abstract model of inverted pendulum on a cart.
 * Safety condition phrased in terms of pitch angle.
 *
 * @provable (see m3_abstract.key.proof)
 */
 
\functions {
	R Theta; // Maximum angular bound
	R a; // abstract parameter (Kp)
	R b; // abstract parameter (Kd)
}

\programVariables {
	R w;	 // Angular velocity
	R theta; // Pitch angle
	R r; // reference angle
}

\problem {
   (  (  (  ((a) <  (0))
          & (((b) ^ (2)) <  ((4) * (-(a)))))
       & ((b) <  (0)))
    & (((-(Theta)) <= (theta)) & ((theta) <= (Theta))))
-> (\[
      ((r := *) ;
        ((?((((((Theta) - (Abs(r))) ^ (2)) * (((4) * (a)) + ((b) ^ (2)))) < ((4) * ((((a) * (((theta) - (r)) ^ (2))) + (((b) * ((theta) - (r))) * (w))) - ((w) ^ (2))))) & (((Theta) - (Abs(r))) > (0)))) ;
          ({(theta') = (w), 
            (w') = (((a) * ((theta) - (r))) + ((b) * (w)))})))*
      \] (  ((-(Theta)) <= (theta))
          & ((theta) <= (Theta))))

}

