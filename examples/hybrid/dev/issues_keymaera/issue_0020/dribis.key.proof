/**
 * Inverted Pendulum on Cart
 *===========================
 *
 * 1-D no-friction model of inverted pendulum on a cart.
 * Safety condition phrased in terms of pitch angle.
 *
 * @provable
 */
 
\functions {
	R Theta;
	R g;
	R l;
}

\programVariables {
	R w;
	R theta;
	R f;
}

\problem {
   (  (  (  (((Theta) >  (0)) & ((g) >  (0)))
          & ((l) = (1)))
       & ((theta) = (0)))
    & ((w) = (0)))
-> (\[
      (f := (((-(g)) / (l)) * (theta))) ;
        ({(theta') = (w), (w') = ((f) + (((g) / (l)) * (theta)))})
      \] (  ((-(Theta)) <= (theta))
          & ((theta) <= (Theta))))

}

