/**
 * Provable with \settings{"[DLOptions]applyToModality=true"}
 * But not provable with \settings{"[DLOptions]applyToModality=false"}
 */
\problem {
  \[ R x1,x2, d1,d2; R om; \] \forall R r; (d1^2+d2^2=r
   ->
   \[ om:=0;
     {x1'=d1,x2'=d2, d1'=om*d2,d2'=-om*d1}
    \] (x1>=0 -> d1^2+d2^2=r)
  )
}
