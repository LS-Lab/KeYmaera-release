\programVariables {
R st, x_1, x_2, y_1, y_2, c_1, c_2, b_1, b_2, u, t, h_1, h_2, ep, x_01, x_02;
}

\problem {
\[c_1 := 10; c_2:=10; b_1:=1/2; b_2:=1/2; u:=10; x_1:=30; x_2:=30; ep := 1/2; h_1:= 1; h_2:=0; st:= 0\] 

\[(if(st = 0) then 
	x_01 := x_1; x_02 := x_2;
	if(x_1 = 20 | x_2 = 20) then st:=1; t:=0 else
	{x_1'=c_1*h_1+b_1*(u-x_1),x_2'=c_2*h_2+b_1*(u-x_2), x_01 >= 20 -> x_1 >= 20, x_02 >= 20 -> x_2 >= 20}
	fi
   else
    (st:=0; if(x_1 > x_2) then h_1 := 0; h_2:=1 else h_1 := 1; h_2:=0 fi) ++ (
    {x_1'=b_1*(u-x_1), x_2'=b_1*(u-x_2), t'=1, t<=ep})
   fi
  )*\] (x_1 >= 15 & x_2 >= 15)


}
