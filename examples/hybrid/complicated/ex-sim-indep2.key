 * @author Andre Platzer
 */
\problem {
  \[ R x1,x2, d1,d2; R y1,y2, e1,e2; R om, c1,c2; c1:=0;c2:=0 \] (
    d1=-om*(x2-c2)&d2=om*(x1-c1)
   &e1=-om*(y2-c2)&e2=om*(y1-c1)
   &(x1-c1)^2+(x2-c2)^2=(y1-c1)^2+(y2-c2)^2
   &!(x1=y1&x2=y2)
  ->
   \[{x1'=d1,x2'=d2}; {y1'=e1,y2'=e2}\] (
        !(x1=y1&x2=y2)
    )
  )
}
